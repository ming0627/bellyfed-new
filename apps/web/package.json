{
  "name": "@bellyfed/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "build:fast": "NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf .next out node_modules/.cache",
    "generate:route": "turbo gen route-web",
    "generate:component": "turbo gen react-component --web"
  },
  "dependencies": {
    "@bellyfed/ui": "workspace:*",
    "@bellyfed/utils": "workspace:*",
    "@bellyfed/trpc": "workspace:*",
    "@bellyfed/db": "workspace:*",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "next": "15.3.2",
    "react": "^18",
    "react-dom": "^18",
    "superjson": "^2.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@turbo/gen": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "postcss": "^8.4.49",
    "sass": "^1.85.1",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.3.3"
  },
  "type": "module"
}
