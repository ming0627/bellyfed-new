{
  "name": "@bellyfed/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:fast": "NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:analyze": "ANALYZE=true pnpm run build",
    "start": "next start",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "fix:ts": "bash scripts/fix-typescript-errors.sh",
    "fix:all": "pnpm run fix:ts && pnpm run lint:fix && pnpm run type-check",
    "quality-check": "pnpm run lint:strict && pnpm run type-check:strict",
    "test": "cp babel.config.js.jest babel.config.js && echo \"No tests specified\" && exit 0 && rm babel.config.js",
    "deploy": "pnpm run build && docker build -t bellyfed-frontend . && docker tag bellyfed-frontend:latest ${ECR_REPOSITORY_URI:-bellyfed-dev}:latest && docker push ${ECR_REPOSITORY_URI:-bellyfed-dev}:latest && aws ecs update-service --cluster ${ECS_CLUSTER_NAME:-bellyfed-dev-cluster} --service ${ECS_SERVICE_NAME:-bellyfed-dev} --force-new-deployment",
    "cache:stats": "node scripts/manage-cache.js stats",
    "cache:clean": "node scripts/manage-cache.js clean",
    "cache:optimize": "node scripts/manage-cache.js optimize",
    "deploy:cloudfront": "pnpm run build && aws s3 sync out/_next/static/css s3://${WEBSITE_BUCKET_NAME}/_next/static/css --delete --cache-control \"public, max-age=31536000, immutable\" --content-type \"text/css\" && aws s3 sync out/_next/static s3://${WEBSITE_BUCKET_NAME}/_next/static --delete --cache-control \"public, max-age=31536000, immutable\" --exclude \"css/*\" && aws s3 sync out/images s3://${WEBSITE_BUCKET_NAME}/images --delete --cache-control \"public, max-age=2592000\" && aws s3 sync out s3://${WEBSITE_BUCKET_NAME} --delete --exclude \"_next/static/*\" --exclude \"images/*\" --cache-control \"public, max-age=0, must-revalidate\" && aws cloudfront create-invalidation --distribution-id ${DISTRIBUTION_ID} --paths \"/*\"",
    "docker:build": "./docker-build.sh",
    "docker:run": "docker run -p 3000:3000 bellyfed-frontend",
    "docker:push": "docker tag bellyfed-frontend:latest ${ECR_REPOSITORY_URI:-bellyfed-dev}:latest && docker push ${ECR_REPOSITORY_URI:-bellyfed-dev}:latest",
    "ecs:deploy": "aws ecs update-service --cluster ${ECS_CLUSTER_NAME:-bellyfed-dev-cluster} --service ${ECS_SERVICE_NAME:-bellyfed-dev} --force-new-deployment",
    "prepare": "node scripts/prepare-husky.js",
    "lint-staged": "lint-staged",
    "pre-commit": "lint-staged",
    "husky:install": "husky install",
    "husky:pre-commit": "husky add .husky/pre-commit 'pnpm run pre-commit'",
    "husky:pre-push": "husky add .husky/pre-push 'pnpm run quality-check'",
    "husky:setup": "pnpm run husky:install && pnpm run husky:pre-commit && pnpm run husky:pre-push",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "next telemetry disable"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.667.0",
    "@aws-sdk/client-s3": "^3.803.0",
    "@aws-sdk/client-secrets-manager": "^3.803.0",
    "@aws-sdk/client-sqs": "^3.675.0",
    "@aws-sdk/client-ssm": "^3.803.0",
    "@aws-sdk/s3-request-presigner": "^3.803.0",
    "@bellyfed/shared": "workspace:*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@mui/material": "^6.2.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.3",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@react-google-maps/api": "^2.20.5",
    "@tanstack/react-query": "^5.62.7",
    "@types/google.maps": "^3.58.1",
    "@types/pg": "^8.11.12",
    "@types/react-datepicker": "^6.2.0",
    "aws-jwt-verify": "^4.0.1",
    "axios": "^1.7.9",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.4.7",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cookie": "^1.0.2",
    "country-region-data": "^3.0.0",
    "country-state-city": "^3.2.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.3.1",
    "framer-motion": "^11.13.3",
    "lucide-react": "^0.441.0",
    "next": "15.3.2",
    "next-auth": "4.24.11",
    "next-themes": "^0.3.0",
    "pg": "^8.14.1",
    "prop-types": "^15.8.1",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-cookie": "^8.0.1",
    "react-datepicker": "^7.4.0",
    "react-day-picker": "^9.4.4",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0",
    "react-select": "^5.8.1",
    "react-select-country-list": "^2.2.3",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.4",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss-animate": "^1.0.7",
    "use-places-autocomplete": "^4.0.1",
    "uuid": "^10.0.0",
    "zod": "^3.22.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "tsc-files --noEmit"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "Dockerfile": [
      "prettier --write"
    ],
    ".dockerignore": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-import-attributes": "^7.27.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.57.0",
    "@next/bundle-analyzer": "^15.3.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/canvas-confetti": "^1.6.4",
    "@types/cookie": "^1.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-select-country-list": "^2.2.3",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "identity-obj-proxy": "^3.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.5.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15",
    "ts-loader": "^9.5.2",
    "tsc-files": "^1.1.4",
    "tsx": "^4.19.1",
    "typescript": "~5.3.3"
  },
  "type": "module"
}
