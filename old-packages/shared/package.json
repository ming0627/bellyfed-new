{
  "name": "@bellyfed/shared",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:fast": "tsc --incremental",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "quality-check": "pnpm run lint:strict && pnpm run type-check:strict",
    "test": "jest",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.667.0",
    "@aws-sdk/client-s3": "^3.803.0",
    "@aws-sdk/client-secrets-manager": "^3.803.0",
    "@aws-sdk/client-sqs": "^3.675.0",
    "@aws-sdk/client-ssm": "^3.803.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@eslint/eslintrc": "^2.1.4",
    "@bellyfed/eslint-config": "workspace:*",
    "@bellyfed/typescript-config": "workspace:*",
    "@types/node": "^20",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "type": "module"
}
